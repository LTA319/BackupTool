{
  "MySqlBackupTool": {
    "ConnectionString": "Data Source=server_backup_tool.db",
    "EnableEncryption": false,
    "EnableNotifications": true,
    "DefaultCompressionLevel": "Fastest",
    "MaxConcurrentBackups": 2,
    "DefaultRetentionDays": 7,
    "EnableAuditing": true,
    "AuditLogPath": "logs/audit/backup-audit-{Date}.log"
  },
  "ServerConfig": {
    "ListenPort": 8080,
    "MaxConcurrentConnections": 10,
    "EnableSsl": false,
    "AuthenticationRequired": false,
    "AllowedClients": [
      "127.0.0.1",
      "192.168.1.0/24"
    ]
  },
  "StorageConfig": {
    "PrimaryStoragePath": "backups",
    "EnableCompression": true,
    "EnableEncryption": false,
    "MaxStorageSize": "10GB",
    "CleanupInterval": "12:00:00"
  },
  "Alerting": {
    "EnableAlerting": true,
    "BaseUrl": "https://api.example.com",
    "TimeoutSeconds": 10,
    "MaxRetryAttempts": 2,
    "EnableCircuitBreaker": false,
    "MinimumSeverity": "Information",
    "MaxAlertsPerHour": 100,
    "NotificationTimeout": "00:00:30",
    "Email": {
      "Enabled": false
    },
    "Webhook": {
      "Enabled": false
    },
    "FileLog": {
      "Enabled": true,
      "LogDirectory": "logs/alerts",
      "FileNamePattern": "alerts_{yyyy-MM-dd}.log",
      "MaxFileSizeMB": 5,
      "MaxFileCount": 7
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "MySqlBackupTool": "Trace",
      "Microsoft": "Information",
      "System": "Information"
    }
  },
  "MemoryProfiling": {
    "EnableProfiling": true,
    "ProfilingInterval": "00:01:00",
    "MaxMemoryThresholdMB": 512,
    "EnableGCCollection": true,
    "LogMemoryUsage": true
  }
}